{
  "name": "node-loadbalance-redis",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:JGGrande/node-loadbalance-redis.git",
  "author": "JGGrande <joaogabriel7303@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev:server": "tsx watch src/shared/infra/server.ts",
    "start": "npx prisma generate && npx prisma migrate deploy && yarn tsup src --out-dir build && node build/shared/infra/server.js",
    "build": "tsup src --out-dir build",
    "test": "npx autocannon -c 500 -d 30 --workers 10 --renderStatusCodes --latency --warmup [-c 2 -d 4] localhost"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "5.0.0",
    "@types/node": "20.4.2",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "inversify": "6.0.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.2",
    "@swc/core": "1.3.70",
    "@types/express": "4.17.17",
    "@types/inversify": "^2.0.33",
    "autocannon": "^7.14.0",
    "prisma": "5.0.0",
    "ts-node": "10.9.1",
    "tsup": "7.1.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  }
}
